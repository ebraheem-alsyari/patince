<p id="notice"><%= notice %></p>

<h1>Regis</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @regis.each do |regi| %>
      <tr>
        <td><%= regi.name %></td>
        <td><%= regi.age %></td>
        <td><%= link_to 'Show', regi %></td>
        <td><%= link_to 'Edit', edit_regi_path(regi) %></td>
        <td><%= link_to 'Destroy', regi, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Regi', new_regi_path %>
